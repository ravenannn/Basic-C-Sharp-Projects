@using CarInsurance.ViewModels
@model List<QuoteVm>


<div class="row no-gutters m3">
    <div class="card col-sm-12 col-md-10 col-lg-6 mx-auto shadow">
        <div class="card-header text-center">
            <h3 class="m-0">Insuree Quotes</h3>
        </div>
        <div class="card-body">
            @foreach (var insuree in Model)
            {
                <input type="hidden" name="Id" value="@insuree.Id" />

                <div class="row">
                    <div class="col mb-2">
                        First Name: 
                        @insuree.FirstName
                    </div>
                    <div class="col ">
                        Last Name: 
                        @insuree.LastName
                    </div>
                    <div class="col mb-4">
                        Email Address:
                        @insuree.EmailAddress
                    </div>
                    <div class="col mb-4">
                        Quote Amount:
                        @insuree.Quote
                        <br />
                        <br />
                    </div>
                </div>
                
            }
        </div>
    </div>
</div>
